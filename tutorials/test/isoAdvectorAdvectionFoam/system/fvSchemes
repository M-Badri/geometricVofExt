FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         none;
}

interpolationSchemes
{
    default         linear;
}

divSchemes
{
    default         none;
}

gradSchemes
{
    default             none;
    grad(alpha.water)   leastSquares;
}

laplacianSchemes
{
    default         none;
}

snGradSchemes
{
    default         none;
}

