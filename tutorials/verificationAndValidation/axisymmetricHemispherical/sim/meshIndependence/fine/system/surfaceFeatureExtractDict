FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

bodySide.stl
{
    extractionMethod    extractFromSurface;
    includedAngle       150;
    writeObj            no;

    subsetFeatures
    {
        nonManifoldEdges    no;
        openEdges           yes;
    }
}

bodyTop.stl
{
    extractionMethod    extractFromSurface;
    includedAngle       150;
    writeObj            no;

    subsetFeatures
    {
        nonManifoldEdges    no;
        openEdges           yes;
    }
}

inlet.stl
{
    extractionMethod    extractFromSurface;
    includedAngle       150;
    writeObj            no;

    subsetFeatures
    {
        nonManifoldEdges    no;
        openEdges           yes;
    }
}

outlet.stl
{
    extractionMethod    extractFromSurface;
    includedAngle       150;
    writeObj            no;

    subsetFeatures
    {
        nonManifoldEdges    no;
        openEdges           yes;
    }
}

side.stl
{
    extractionMethod    extractFromSurface;
    includedAngle       150;
    writeObj            no;

    subsetFeatures
    {
        nonManifoldEdges    no;
        openEdges           yes;
    }
}

